<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trh.dotgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TitleActivity"
            android:label="DotGame">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN" />
                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StoryActivity"
            android:label="DotGame"></activity>
        <activity android:name=".FreePlayActivity"
            android:label="FreePlay"></activity>

    </application>

</manifest>
